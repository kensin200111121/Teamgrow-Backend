{
  "name": "teamgrow",
  "version": "1.0.0",
  "description": "Teamgrow backend",
  "main": "app.js",
  "scripts": {
    "setup:dev": "./.commands/dev.sh crmgrow-packages @teamgrow/common",
    "start": "node --max_old_space_size=8000 ./bins/www.js",
    "stage": "NODE_ENV=stage node --max_old_space_size=8000 ./bins/www.js",
    "dev": "nodemon --max_old_space_size=8000 --trace-warnings ./bins/www.js",
    "lint": "eslint '*.js' --quiet --fix",
    "format": "prettier '*.js' --write",
    "lint-fix": "bash fixTab.sh && eslint config controllers models routes --fix",
    "deploy": "pm2 deploy ecosystem.config.json stage --force",
    "test": "jest --verbose=false --silent=false",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "migrate:init": "migrate-mongo init",
    "migrate:create": "migrate-mongo create",
    "migrate:status": " migrate-mongo status",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "swagger": "swagger-merger -i src/swagger/index.yaml -o swagger.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamgrow/backend_admin.git"
  },
  "keywords": [
    "teamgrow",
    "backend"
  ],
  "author": "superwebtop",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teamgrow/backend_admin/issues"
  },
  "homepage": "https://github.com/teamgrow/backend_admin#readme",
